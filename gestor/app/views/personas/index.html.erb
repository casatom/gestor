<p id="notice"><%= notice %></p>

<h1>Mostrando Personas</h1>

<table>
  <thead>
    <tr>
      <th>Dni</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personas.each do |persona| %>
      <tr>
        <td><%= persona.dni %></td>
        <td><%= persona.nombre %></td>
        <td><%= persona.apellido %></td>
      </tr>
    <% end %>
  </tbody>
</table>
